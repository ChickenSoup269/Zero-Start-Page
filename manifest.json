{
  "manifest_version": 3,
  "name": "Startpage",
  "version": "1.0",
  "description": "A beautiful and simple startpage replacement.",
  "permissions": ["bookmarks", "scripting", "tabs"],
  "icons": {
    "128": "icon/logo.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "https://suggestqueries.google.com/*",
    "*://*.youtube.com/*",
    "*://*.spotify.com/*",
    "*://music.youtube.com/*",
    "*://images.google.com/*"
  ]
}
